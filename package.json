{
  "name": "@fms-cat/automaton-with-gui",
  "version": "3.0.0-beta5",
  "description": "Animation engine for creative coding, with GUI!",
  "main": "dist/automaton-with-gui.min.js",
  "repository": "https://github.com/FMS-Cat/automaton-with-gui",
  "author": "FMS-Cat",
  "types": "types/index.d.ts",
  "typesVersions": {
    "<3.8": {
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development SERVE=1 rollup -w -c -o dist/automaton-with-gui.js",
    "all": "yarn lint && yarn test && yarn clean && yarn build && yarn docs",
    "version": "yarn all",
    "clean": "rimraf dist docs ts*/ types",
    "build": "yarn build-dev && yarn build-prod && yarn build-esm && yarn build-esmprod && yarn build-types",
    "build-dev": "cross-env NODE_ENV=development rollup -c -o dist/automaton-with-gui.js",
    "build-prod": "cross-env NODE_ENV=production rollup -c -o dist/automaton-with-gui.min.js",
    "build-esm": "cross-env NODE_ENV=development ESM=1 rollup -c -o dist/automaton-with-gui.module.js",
    "build-esmprod": "cross-env NODE_ENV=production ESM=1 rollup -c -o dist/automaton-with-gui.module.min.js",
    "build-types": "tsc --declaration --declarationDir ./types --emitDeclarationOnly && downlevel-dts types ts3.4/types",
    "docs": "typedoc --out docs --mode file --excludeNotExported",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "files": [
    "/dist/",
    "/ts*/",
    "/types/",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "dependencies": {
    "@fms-cat/automaton": "3.0.0-beta7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@svgr/rollup": "^5.4.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "downlevel-dts": "^0.6.0",
    "eslint": "^7.7.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eventemitter3": "^4.0.7",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "gh-pages": "^3.1.0",
    "git-branch-is": "^4.0.0",
    "immer": "^7.0.8",
    "jest": "^26.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.8",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-terser": "^7.0.1",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
