{
  "opts": {
    "template": "node_modules/boxy-jsdoc-template", 
    "destination": "./docs",
    "recurse": true,
    "private": false
  },
  "templates": {
    "default": {
      "useLongnameInNav": true,
      "includeDate": false
    }
  },
  "plugins": [ "plugins/markdown" ],
  "markdown": {
    "hardwrap": "true"
  }
}